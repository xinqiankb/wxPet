/**app.wxss**/
.wrap{margin: 0; padding: 0;padding: 10px;}
view{font-family: "Microsoft YaHei";font-size: 1rem;}
@font-face {
  font-family: 'iconfont';  /* project id 505145 */
  src: url('//at.alicdn.com/t/font_505145_7gk0cuhgbs1kmx6r.eot');
  src: url('//at.alicdn.com/t/font_505145_7gk0cuhgbs1kmx6r.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_505145_7gk0cuhgbs1kmx6r.woff') format('woff'),
  url('//at.alicdn.com/t/font_505145_7gk0cuhgbs1kmx6r.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_505145_7gk0cuhgbs1kmx6r.svg#iconfont') format('svg');
